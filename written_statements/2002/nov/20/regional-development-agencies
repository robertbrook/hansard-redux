          <table>
          <tr>
          <td>GLOSSARY OF TERMS</td>
          <td>ANNEX A</td>
          </tr>
          <tr>
          <td><span class="italic">Term</span></td>
          <td><span class="italic">Definition</span></td>
          </tr>
          <tr>
          <td>Corporate Plan</td>
          <td>RDA plans which set out how resources will be used to achieve the objectives set out in their Regional Economic Strategy.</td>
          </tr>
          <tr>
          <td>Gross</td>
          <td>All outputs directly attributable to projects or programmes</td>
          </tr>
          <tr>
          <td>Gross Attributable</td>
          <td>Gross outputs that are directly attributable to RDA input, i.e. the RDA share of the outputs.</td>
          </tr>
          <tr>
          <td>SWDA</td>
          <td>South West Development Agency</td>
          </tr>
          <tr>
          <td>EEDA</td>
          <td>East of England Development Agency</td>
          </tr>
          </table>
          
