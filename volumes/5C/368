ays-and-means#column_330">330</a></td><td class="column-number"><a href="/commons/1941/jan/23/ways-and-means#column_331">331</a></td><td class="column-number"><a href="/commons/1941/jan/23/ways-and-means#column_332">332</a></td><td class="column-number"><a href="/commons/1941/jan/23/ways-and-means#column_333">333</a></td><td class="column-number"><a href="/commons/1941/jan/23/ways-and-means#column_334">334</a></td><td class="column-number"><a href="/commons/1941/jan/23/ways-and-means#column_335">335</a></td><td class="column-number"><a href="/commons/1941/jan/23/ways-and-means#column_336">336</a></td><td class="column-number"><a href="/commons/1941/jan/23/war-damage-bill#column_337">337</a></td><td class="column-number"><a href="/commons/1941/jan/23/war-damage-bill#column_338">338</a></td><td class="column-number"><a href="/commons/1941/jan/23/war-damage-bill#column_339">339</a></td></tr><tr><td class="column-number"><a href="/commons/1941/jan/23/war-damage-bill#column_340">340</a></td><td class="column-number"><a href="/commons/1941/jan/23/war-damage-bill#column_341">341</a></td><td class="column-number"><a href="/commons/1941/jan/23/war-damage-bill#column_342">342</a></td><td class="column-number"><a href="/commons/1941/jan/23/war-damage-bill#column_343">343</a></td><td class="column-number"><a href="/commons/1941/jan/23/war-damage-bill#column_344">344</a></td><td class="column-number"><a href="/commons/1941/jan/23/war-damage-bill#column_345">345</a></td><td class="column-number"><a href="/commons/1941/jan/23/war-damage-bill#column_346">346</a></td><td class="column-number"><a href="/commons/1941/jan/23/war-damage-bill#column_347">347</a></td><td class="column-number"><a href="/commons/1941/jan/23/war-damage-bill#column_348">348</a></td><td class="column-number"><a href="/commons/1941/jan/23/war-damage-bill#column_349">349</a></td></tr><tr><td class="column-number"><a href="/commons/1941/jan/23/war-damage-bill#column_350">350</a></td><td class="column-number"><a h